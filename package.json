  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danilo-vieira/node-typescript-setup.git"
  },
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify --inspect=0.0.0.0 src/shared/infra/http/server.ts",
    "test": "NODE_ENV=test jest --verbose --runInBand --detectOpenHandles",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "seed:admin": "ts-node-dev -r tsconfig-paths/register ./src/shared/infra/typeorm/seed/admin.ts"
  },
  "dependencies": {
    "aws-sdk": "^2.983.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "cors": "^2.8.5",
    "csv-parse": "^4.16.0",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.5.2",
    "multer": "^1.4.3",
    "nodemailer": "^6.6.3",
    "pg": "^8.7.1",
    "rate-limiter-flexible": "^2.2.4",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
